{
  "name": "dotastats-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "node-sass": "^4.5.0",
    "history": "^4.6.0",
    "material-ui": "^0.17.1",
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.11",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-moment": "^0.2.2",
    "react-redux": "^5.0.3",
    "react-router": "3.0.2",
    "react-router-redux": "^4.0.8",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "react-scripts": "0.9.5"
  },
  "devDependencies": {
    "npm-run-all": "^4.0.2",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "redux-logger": "^2.8.2"
  },
  "scripts": {
    "build-css": "node-sass public/assets/css/style.scss -o public/assets/css/",
    "watch-css": "npm run build-css && node-sass --watch --recursive public/assets/css/ -o public/assets/css/",
    "start": "REACT_APP_ENV=production npm run build-css && serve",
    "serve": "react-script start",
    "start-dev": "npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
