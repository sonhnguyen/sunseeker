{
	"parser": "babel-eslint",
	"extends": [
		"eslint:recommended",
		"plugin:import/recommended",
		"plugin:react/recommended"
	],
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true,
			"experimentalObjectRestSpread": true
		},
	},
	"env": {
		"browser": true,
		"mocha": true,
		"node": true
	},
	"plugins": [
		"react",
		"import"
	],
	"rules":{
		"import/unambiguous": 0
	}
}
